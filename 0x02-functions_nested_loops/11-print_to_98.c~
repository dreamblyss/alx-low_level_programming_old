#include <stdio.h>
#include "main.h"
/**
 * print_to_98 - a function that prints all natural numbers from n to 98
 * user input's number prints to 98, regardless < 98 or > 98
 * @num: number input
 * Return: Always 0 (Success)
 */
void print_to_98(int num)
{
if (num <= 98)
{
for (int n = num; n <= 98; n++)
{
if (n != 98)
{
printf("%d, ", n);
}
else if (n == 98)
{
  prinf("%d\n", n);
}  
}    
}  
}
